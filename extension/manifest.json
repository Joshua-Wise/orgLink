{
  "manifest_version": 3,
  "name": "orgLink",
  "version": "1.0",
  "description": "Quick access to organization resource links",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
  {
    "resources": ["links.json"],
    "matches": ["<all_urls>"]
  }
]
}